{
    "name": "innmind/neo4j-dbal",
    "type": "library",
    "description": "Abstraction layer of the Neo4j http API",
    "keywords": ["neo4j", "wrapper"],
    "homepage": "http://github.com/Innmind/neo4j-dbal",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/neo4j-dbal/issues"
    },
    "require": {
        "php": "~7.2",
        "innmind/immutable": "~2.0",
        "psr/log": "^1.0",
        "innmind/time-continuum": "~1.0",
        "innmind/http-transport": "~4.0",
        "innmind/json": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Neo4j\\DBAL\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Neo4j\\DBAL\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "giorgiosironi/eris": "^0.10.0 || ^0.11.0",
        "innmind/cli": "^1.5",
        "innmind/object-graph": "^1.2",
        "innmind/server-control": "^2.7"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    }
}
